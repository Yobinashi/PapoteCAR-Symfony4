{% extends 'base.html.twig' %}

{% block title %} Users {% endblock %}

{% block body %}
    <h1> USERS </h1>
    <ul>
    {% for user in users %}
      <li><a href="{{ path('userProfile', {'id':user.id}) }}">{{ user.firstname }} {{ user.lastname }}</a></li>
    {% else %}
        No users
    {% endfor %}
    </ul>
{% endblock %}


